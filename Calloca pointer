#include <stdio.h>
#include <stdlib.h>

int main() {
    int *ptr;
    int i;
    ptr = (int*)calloc(5, sizeof(int));
     if(ptr == NULL) {
        printf("Memory not allocated!\n");
        return 0;
    }
    printf("Values in allocated memory:\n");
    for(i = 0; i < 5; i++) {
        printf("%d ", ptr[i]);
    }
    free(ptr);
    return 0;
}
