#include <stdio.h>

int main() {
    int a, b;

    // Taking input from user
    printf("Enter value of a: ");
    scanf("%d", &a);

    printf("Enter value of b: ");
    scanf("%d", &b);

    // Swapping without temporary variable
    a = a + b;
    b = a - b;
    a = a - b;

    // Displaying results
    printf("After swapping:\n");
    printf("a = %d\n", a);
    printf("b = %d\n", b);

    return 0;
}
