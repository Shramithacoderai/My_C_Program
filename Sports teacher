#include <stdio.h>

int main() {
    int n, i, j, temp;

    printf("Enter number of students: ");
    scanf("%d", &n);

    int scores[n];

    // Input scores
    printf("Enter the scores:\n");
    for(i = 0; i < n; i++) {
        scanf("%d", &scores[i]);
    }

    // Sorting in descending order (Bubble Sort)
    for(i = 0; i < n - 1; i++) {
        for(j = 0; j < n - i - 1; j++) {
            if(scores[j] < scores[j + 1]) {
                temp = scores[j];
                scores[j] = scores[j + 1];
                scores[j + 1] = temp;
            }
        }
    }

    // Display sorted scores
    printf("\nScores in Descending Order:\n");
    for(i = 0; i < n; i++) {
        printf("%d ", scores[i]);
    }

    return 0;
}
