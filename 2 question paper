#include <stdio.h>
int main(){
    int Marks;
    printf("Enter marks:\n");
    scanf("%d",&Marks);
if (Marks>=90) {
    printf("Grade A\n");
}
else if (Marks>=75) {
    printf("Grade B\n");
}
else if (Marks>=60) {
    printf("Grade C\n");
}
else if (Marks>=50) {
    printf("Grade D\n");
}
else {
    printf("Grade F\n");
}
}
